{
    "name": "yipack-template",
    "version": "1.0.0",
    "description": "yipack-cli脚手架项目模板",
    "main": "index.js",
    "scripts": {
        "dev": "yipack dev",
        "build": "yipack build",
        "build:analyzer": "yipack build --analyzer",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "browserslist": [
        "> 1%",
        "ie > 8",
        "last 2 versions",
        "last 3 iOS versions"
    ],
    "keywords": [
        "yipack",
        "yipack-cli",
        "yipack template",
        "demo",
        "template",
        "example"
    ],
    "author": "chensuiyi <bimostyle@qq.com>",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.21.1",
        "basil.js": "^0.4.11",
        "flat": "^5.0.2",
        "path-browserify": "^1.0.1",
        "vue": "^2.6.12",
        "vue-i18n": "^8.22.4",
        "vue-router": "^3.5.1",
        "vuex": "^3.6.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,css,scss,less,ts,jsx,vue,html,json,md,yaml}": "prettier --write"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-angular": "^11.0.0",
        "conventional-changelog-cli": "^2.1.1",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1"
    }
}
